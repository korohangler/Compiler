{
    "Rules":
    [
        {
            "Regexp" : "\".*\"",
            "Type"   : "ConstantString"
        },
        {
            "Regexp" : "\/\\*([^*]|[\r\n]|(\\*+([^*/]|[\r\n])))*\\*\/+",
            "Type"   : "Comment"
        },
        {
            "Regexp" : "\/\/.*\n",
            "Type"   : "Comment"
        },
        {
            "Regexp" : "(;|\r\n|\r|\n|\\s|$)",
            "Type"   : "Separator",
            "DeleteDuplicates" : true,
            "NeedExport" : false
        },
        {
            "Regexp" : "[-+]?([0-9]*|.[0-9]+|[0-9]+.|[0-9]+)[eE]?[-+]?[0-9]+[flFL]?",
            "Type"   : "DoubleLiteral"
        },
        {
            "Regexp" : "(let)|(var)",
            "Type"   : "variableDeclaration"
        },
        {
            "Regexp" : "=",
            "Type"   : "Equal"
        },
        {
            "Regexp" : "(\\w[\\d\\w_]?)",
            "Type"   : "Identificator"
        }
    ]
}
