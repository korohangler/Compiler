{
    "Rules":
    [
        {
            "Regexp" : "\".*\"",
            "Type"   : "ConstantString"
        },
        {
            "Regexp" : "\/\\*([^*]|[\r\n]|(\\*+([^*/]|[\r\n])))*\\*\/+",
            "Type"   : "Comment"
        },
        {
            "Regexp" : "\/\/.*\n",
            "Type"   : "Comment"
        },
        {
            "Regexp" : "(;|\r\n|\r|\n|\\s|$)",
            "Type"   : "Separator"
        },
        {
            "Regexp" : "[-|+]?([0-9]*.[0-9]+|[0-9]+.|[0-9]+)[e|E]?[-|+]?[0-9]+[flFL]?",
            "Type"   : "Digit"
        },
        {
            "Regexp" : "(let)|(var)",
            "Type"   : "variableDeclaration"
        },
        {
            "Regexp" : "=",
            "Type"   : "Equal"
        },
        {
            "Regexp" : "\\w",
            "Type"   : "Identificator"
        }
    ]
}
